  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/jest": "^28.1.7",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
